{
  "name": "ts-react-starter",
  "version": "1.0",
  "description": "Starter for TS React projects",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:constnazarenko/ts-react-starter.git"
  },
  "keywords": [
    "ts",
    "starter"
  ],
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src/*",
    "dist/*"
  ],
  "author": {
    "name": "Kostiantyn Nazarenko",
    "email": "const@nazarenko.es"
  },
  "homepage": "https://github.com/constnazarenko/ts-react-starter",
  "private": false,
  "dependencies": {
    "classnames": "2.3.1",
    "moment": "2.29.2",
    "react-redux": "7.2.8",
    "redux": "4.1.2",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.24",
    "@types/redux-logger": "3.0.9",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "mini-css-extract-plugin": "2.6.0",
    "npm-check": "5.9.2",
    "prettier": "2.6.2",
    "prettier-plugin-organize-imports": "2.3.4",
    "react": "17.0.2",
    "react-bootstrap": "1.6.4",
    "react-dom": "17.0.2",
    "sass": "1.50.0",
    "sass-loader": "12.6.0",
    "source-map-explorer": "2.5.2",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "stylelint": "14.8.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-sass-guidelines": "9.0.1",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.8.1",
    "webpack-merge": "5.8.0"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-bootstrap": ">=1.6.4",
    "react-dom": ">=18.2.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "check-types": "tsc",
    "compile": "tsc -p .",
    "posttest": "npm run check-types",
    "predeploy": "npm run build",
    "prepare": "npm run compile && husky install",
    "pretest": "npm run compile",
    "start": "webpack serve --config webpack.dev.js",
    "test": "npm run compile",
    "analyze": "source-map-explorer 'dist/*.js' --html bundle-analysis.html",
    "upgrade-interactive": "npm-check --update --save-exact",
    "linter": "eslint src --ext .js,.jsx,.ts,.tsx",
    "linter-fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write --ignore-unknown src/ translations/ ./package.json ./README.md",
    "prettier-check": "prettier --check src/ translations/ ./package.json ./README.md"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": "stylelint --fix",
    "*.{css,scss,md}": "prettier --write"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">12"
  }
}
